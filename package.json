{
  "name": "game_goblin",
  "version": "1.0.0",
  "description": "A website to show off my WebAssembly games",
  "main": "index.js",
  "scripts": {
    "webpack-build": "webpack --mode production --entry ./_build/default/src/output/src/Entry.js -o ./public",
    "dev": "webpack serve --mode development --port 4321 --entry ./_build/default/src/output/src/Entry.js",
    "sass-build": "sass src/scss/main.scss public/main.css",
    "sass-watch": "sass src/scss/main.scss public/main.css -w",
    "bundle": "npm run sass-build && npm run webpack-build"
  },
  "author": "lboyle@protonmail.com",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/themes": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "sass": "^1.72.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
